    "model:generate": "node scripts/generateModel.js",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate --config src/config/config.json --migrations-path src/migrations",
    "db:migrate:undo": "sequelize db:migrate:undo --config src/config/config.json --migrations-path src/migrations",
    "db:seed": "sequelize db:seed:all",
    "db:drop": "sequelize db:drop",
    "db:info": "sequelize db:info",
    "db:seed:undo": "sequelize db:seed:undo"
  },
